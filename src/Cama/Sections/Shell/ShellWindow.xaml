<Window x:Class="Cama.Sections.Shell.ShellWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:infrastructure="clr-namespace:Cama.Infrastructure;assembly=Cama.Infrastructure"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <ContentControl prism:RegionManager.RegionName="{x:Static infrastructure:RegionNames.MainContentRegion}" Grid.Row="1" />
    </Grid>
</Window>
